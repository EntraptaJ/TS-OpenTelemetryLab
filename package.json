{
  "name": "ts-core",
  "version": "0.0.1",
  "description": "TS-Core is a github repo template for TypeScript Projects",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "echo TODO",
    "prepublishOnly": "npm run build",
    "build": "tsc -p ./tsconfig.build.json",
